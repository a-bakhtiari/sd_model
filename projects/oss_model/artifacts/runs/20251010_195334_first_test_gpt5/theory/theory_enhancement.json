{
  "theories": [
    {
      "name": "Communities of Practice (Wenger)",
      "rationale": "CoP emphasizes newcomer trajectories from peripheral to core participation, mutual engagement, and a shared repertoire that sustains learning. The current model captures mentorship and progression but lacks mechanisms for retention vs. dropout, psychological safety, onboarding friction, and the practices/rituals that stabilize community identity. Adding these elements clarifies how participation conditions translate into learning and retention, reinforcing feedbacks that grow or erode the community.",
      "additions": {
        "variables": [
          {
            "name": "Psychological Safety",
            "type": "Auxiliary",
            "description": "Perceived safety for newcomers and contributors to ask, fail, and participate without negative repercussions."
          },
          {
            "name": "Onboarding Friction",
            "type": "Auxiliary",
            "description": "Practical and social barriers for newcomers to contribute (e.g., unclear setup, norms, tooling)."
          },
          {
            "name": "Newcomer Dropout Rate",
            "type": "Flow",
            "description": "Rate at which New Contributors leave the community before progressing."
          },
          {
            "name": "Mentoring Capacity",
            "type": "Auxiliary",
            "description": "Effective capacity for mentorship and review support provided by core and experienced contributors given available time."
          },
          {
            "name": "Community Practices and Rituals",
            "type": "Stock",
            "description": "Accumulated shared practices (onboarding sessions, review norms, meetings) that support mutual engagement."
          },
          {
            "name": "Practice Formation Rate",
            "type": "Flow",
            "description": "Rate of creating or institutionalizing new community practices and rituals."
          },
          {
            "name": "Practice Decay Rate",
            "type": "Flow",
            "description": "Rate at which practices/rituals become inactive or are abandoned."
          }
        ],
