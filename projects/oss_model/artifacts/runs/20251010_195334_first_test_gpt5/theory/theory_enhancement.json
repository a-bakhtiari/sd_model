    {
      "name": "Nonaka's SECI Model",
      "rationale": "SECI requires explicit representation of socialization, externalization, combination, and internalization dynamics. The model includes mentorship and some conversion flows, but it lacks combination/curation, explicit knowledge quality/obsolescence, discovery/use effectiveness, and a backlog that channels conversion efforts. These additions complete the spiral and make knowledge flows sensitive to production capacity and artifact quality.",
      "additions": {
        "variables": [
          {
            "name": "Documentation Backlog",
            "type": "Stock",
            "description": "Accumulated undocumented insights and needed docs/tutorials derived from issues, PRs, and feedback."
          },
          {
            "name": "Documentation Production Rate",
            "type": "Flow",
            "description": "Rate of producing or updating documentation that reduces the Documentation Backlog and increases explicit knowledge."
          },
          {
            "name": "Knowledge Artifact Quality",
            "type": "Auxiliary",
            "description": "Average quality, clarity, and usability of explicit knowledge artifacts."
          },
          {
            "name": "Knowledge Discovery Effectiveness",
            "type": "Auxiliary",
            "description": "Effectiveness with which contributors find and apply relevant explicit knowledge."
          },
          {
            "name": "Combination/Curation Rate",
            "type": "Flow",
            "description": "Rate of synthesizing, organizing, and integrating existing explicit knowledge into higher-value artifacts."
          },
          {
            "name": "Explicit Knowledge Obsolescence Rate",
            "type": "Flow",
            "description": "Rate at which explicit knowledge becomes outdated and is removed or discounted."
          },
          {
            "name": "Documentation Tooling Maturity",
            "type": "Auxiliary",
            "description": "Maturity of tools/processes for authoring, reviewing, and publishing documentation."
          }
        ],
