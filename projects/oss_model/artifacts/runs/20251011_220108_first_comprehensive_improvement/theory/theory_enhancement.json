    {
      "name": "The Coevolution of New Organizational Forms",
      "rationale": "Projects coevolve their processes and tools in response to ecosystem pressures (users, issue load) and internal culture. Adding process/tooling maturity stocks with adaptation/decay flows, driven by ecosystem pressure, captures this coevolution and its impact on throughput and delay.",
      "additions": {
        "variables": [
          {
            "name": "Contribution Process Maturity",
            "type": "Stock",
            "description": "Maturity of contribution workflows (templates, CI, checklists, automation)."
          },
          {
            "name": "Process Adaptation Rate",
            "type": "Flow",
            "description": "Rate of improving or updating contribution processes."
          },
          {
            "name": "Process Decay Rate",
            "type": "Flow",
            "description": "Rate at which processes become outdated or are bypassed."
          },
          {
            "name": "Tooling Maturity",
            "type": "Stock",
            "description": "Maturity of project tooling (CI/CD, bots, linters, triage automation)."
          },
          {
            "name": "Tooling Investment Rate",
            "type": "Flow",
            "description": "Rate of investing in or adopting new tooling."
          },
          {
            "name": "Tooling Obsolescence Rate",
            "type": "Flow",
            "description": "Rate at which existing tooling becomes obsolete or breaks."
          },
          {
            "name": "Ecosystem Pressure",
            "type": "Auxiliary",
            "description": "External/internal demand to change processes and tools."
          }
        ],
        "connections": [
          {
            "from": "Process Adaptation Rate",
            "to": "Contribution Process Maturity",
