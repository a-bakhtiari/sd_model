{
  "theories": [
    {
      "name": "Communities of Practice (Wenger)",
      "rationale": "The model already captures movement from newcomer to core via skill up and promotion, plus mentorship. It lacks explicit levers for mentoring capacity, peer support among newcomers, and identity-based retention. Adding these clarifies how social participation and feedback shape learning trajectories and retention, strengthening CoP dynamics.",
      "additions": {
        "variables": [
          {
            "name": "Newcomer Dropout Rate",
            "type": "Flow",
            "description": "Rate at which New Contributors disengage due to poor socialization or friction."
          },
          {
            "name": "Core Developer Mentoring Capacity",
            "type": "Auxiliary",
            "description": "Effective capacity of core developers to mentor newcomers given time and numbers."
          },
          {
            "name": "Peer Support Among Newcomers",
            "type": "Auxiliary",
            "description": "Degree of mutual aid and knowledge sharing among newcomers."
          },
          {
            "name": "Community Identity Strength",
            "type": "Auxiliary",
            "description": "Sense of belonging and identification with the project community."
          }
        ],
        "connections": [
          {
