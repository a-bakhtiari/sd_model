{
  "variables": [
    "Adopters",
    "Adoption Rate",
    "Attrition",
    "New Adopters",
    "Potential Adopters"
  ],
  "equations": {
    "Potential Adopters": "1000 - Adopters",
    "New Adopters": "Adoption Rate * Potential Adopters",
    "Adopters": "INTEG(New Adopters - Attrition, 0)",
    "Adoption Rate": "0.05",
    "Attrition": "0.0"
  }
}